{
    "manifest_version": 3,
    "name": "ReplitGPT",
    "description": "Copy code from ChatGPT to Replit!",
    "version": "1.0",
    "content_scripts": [
        {
            "run_at": "document_end",
            "js": ["content.js"],
            "matches": [
                "https://replit.com/new/*",
                "https://chat.openai.com/*"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16":"icon.png",
        "19": "icon.png",
        "38": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}